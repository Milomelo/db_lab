package chattingcode;

import javax.swing.JFrame;

public class ChattFrame extends JFrame {

	public ChattFrame() {
setTitle("MyChat1.0");
		setSize(300, 300);
		setLocationRelativeTo(null);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setVisible(true);
	}	

	public static void main(String[] args) {
		new ChattFrame();
	}
}
