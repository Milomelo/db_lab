package chattingcode;

import javax.swing.JFrame;

public class ChattFrame extends JFrame {

	public ChattFrame() {

		initSetting();
		setVisible(true);
	}

	private void initSetting() {
		setSize(300, 300);
		setLocation(null);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	}

	public static void main(String[] args) {
		new ChattFrame();
	}
}
