package chattingcode;

import javax.swing.JFrame;

public class ChattFrame extends JFrame {

	public ChattFrame() {

		setSize(300, 300);
		setLocation(null);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setVisible(true);
	}	

	public static void main(String[] args) {
		new ChattFrame();
	}
}
